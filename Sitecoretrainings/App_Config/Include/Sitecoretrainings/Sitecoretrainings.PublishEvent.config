<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <events>
      <event name="publish:end">
        <handler type="Sitecoretrainings.Events.PublishEventHandler, Sitecoretrainings" method="OnPublishEnd"/>
      </event>
      <event name="publish:itemProcessed">
        <handler type="Sitecoretrainings.Events.PublishEventHandler, Sitecoretrainings" method="OnItemPublished"/>
      </event>
      <event name="publish:itemProcessing">
        <handler type="Sitecoretrainings.Events.PublishEventHandler, Sitecoretrainings" method="OnItemPublishing"/>
      </event>
    </events>
  </sitecore>
</configuration>
